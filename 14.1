#include <stdio.h>
#include <math.h>
#include <locale.h>


int main()
{
    setlocale(LC_ALL, "Rus");
    int a, b, i, j;

    printf("Введите a\n");
    scanf_s("%d", &a);
    printf("Введите b\n");
    scanf_s("%d", &b);

    for (i = a; i <= b; i++)
    {
        for (j = 0; j < i; j++)
        {
            printf("%d", i);
        }
        printf("\n");
    }
    return 0;
}
