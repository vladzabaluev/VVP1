#include <stdio.h>
#include <math.h>
#include <locale.h>


int main()
{
    setlocale(LC_ALL, "Rus");
    int n, i, a, d;

    printf("Введите N \n");
    scanf_s("%d", &n);
    printf("Введите A \n");
    scanf_s("%d", &a);
    printf("Введите D \n");
    scanf_s("%d", &d);

    int* mass = new int[n];
    for (i = 0, a; i < n; i++, a *= d)
    {
        mass[i] = a;
        printf("%d ", mass[i]);
    }


    return 0;
}
