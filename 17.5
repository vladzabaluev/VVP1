#include <stdio.h>
#include <math.h>
#include <locale.h>


int main()
{
    setlocale(LC_ALL, "Rus");
    int n, i, j, m1, m2, a;

    printf("Введите N \n");
    scanf_s("%d", &n);

    int* mass = new int[n];
    for (i = 0; i < n; i++)
    {
        printf("Введите элемент массива \n");
        scanf_s("%d", &a);
        mass[i] = a;
    }
    printf("\n");
    m2 = -1;
    j = 0;
    while (m2 == -1) 
    {
        m1 = mass[j];
        for (i = 0; i < n; i++) 
        {
            if (m1 == mass[i] && i!=j)
            {
                m2 = i;
            }
        }
        j++;
    }
    for (i = 0; i < n; i++) 
    {
        if (mass[i] == mass[m2] && m2!=i) 
        {
            m1 = i;
        }
    }
    printf("%d ", m1);
    printf("%d", m2);

    return 0;

}
