#include <stdio.h>
#include <math.h>
#include <locale.h>

int main()
{
    setlocale(LC_ALL, "Rus");
    int a;

    printf("Введите кол-во дней прошедших сначала года \n");
    scanf_s("%d", &a);

    while (a >= 7)
    {
        a -= 7;
    }
    printf("Номер недели для К-го дня года %d", a);
    return 0;
}
