#include <stdio.h>
#include <math.h>
#include <locale.h>

int main()
{
    setlocale(LC_ALL, "Rus");
    int a, b, c;

    printf("Введите a,b,c \n");
    scanf_s("%d", &a);
    scanf_s("%d", &b);
    scanf_s("%d", &c);

    if (a < b && b < c)
    {
        printf("Высказывание a<b<c верно");
    }
    else 
        printf("Высказывание a<b<c неверно");

    return 0;

}
