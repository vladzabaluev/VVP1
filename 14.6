#include <stdio.h>
#include <math.h>
#include <locale.h>


int main()
{
    setlocale(LC_ALL, "Rus");
    int a, b = 0, c = 1, t, i = 0;

   
    printf("Введите N\n");
    scanf_s("%d", &a);

    while (b != a) 
    {
        t = b;
        b += c;
        c = t;
        i++;
    }
    printf("%d", i+1);
    return 0;
}
