#include <stdio.h>
#include <math.h>
#include <locale.h>

int main()
{
    setlocale(LC_ALL, "Rus");
    int a, b;

    printf("Введите а и б\n");
    scanf_s("%d", &a);
    scanf_s("%d", &b);

    if (a > 2 && b <= 3)
    {
        printf("Высказывание  A > 2 и B <= 3 истинно");
    }
    else
        printf("Высказывание A > 2 и B <= 3 не истинно");
    return 0;

}
