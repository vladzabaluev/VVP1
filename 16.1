#include <stdio.h>
#include <math.h>
#include <locale.h>


int main()
{
    setlocale(LC_ALL, "Rus");
    int n, i, j;

    printf("Введите N \n");
    scanf_s("%d", &n);
    
    int* a = new int[n];
    for (i = 0, j = 1; i < n; i++, j += 2)
    {
        a[i] = j;
        printf("%d ", a[i]);
    }


    return 0;
}
