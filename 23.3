#include <iostream>
#include <string>     
#include <windows.h> 

using namespace std;

int main()
{
    SetConsoleCP(1251);
    SetConsoleOutputCP(1251);

    cout << "Введите строку" << endl;
    string str;
    getline(cin, str);
    int a = 0;
    for (int i = 0; i < str.length(); i++) 
    {
        if ('A' <= str[i] && 'Z' >= str[i])
        {
            a++;
        }
    }
    cout << "Количество прописных латинских букв равно: " << a;
}
