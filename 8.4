#include <stdio.h>
#include <math.h>
#include <locale.h>

int main()
{
    setlocale(LC_ALL, "Rus");
    int a, b;

    printf("Введите двузначное число \n");
    scanf_s("%d", &a);

    b = a % 10;
    a /= 10;
    a = b * 10 + a;

    printf("Число полученное при перестановке цифр %d", a);
    return 0;
}
