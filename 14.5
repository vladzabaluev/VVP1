#include <stdio.h>
#include <math.h>
#include <locale.h>


int main()
{
    setlocale(LC_ALL, "Rus");
    int a, b;

    printf("Введите a\n");
    scanf_s("%d", &a);
    printf("Введите b\n");
    scanf_s("%d", &b);

    while (a != 0 && b != 0 )
    {

        if (a > b) 
        {
            a -= b;
        }
        else
        {
            b -= a;
        }
    }
    printf("%d", a);
    return 0;
}
