#include <stdio.h>
#include <math.h>
#include <locale.h>

void PowerA3( float A)
{
    float B;
    B = A * A * A;
    printf("%f", A);
    printf(" в 3 степени равно %f\n", B);
}
int main()
{
    setlocale(LC_ALL, "Rus");
    int a;

    for (int i = 0; i < 5; i++) 
    {
        printf("Введите A\n");
        scanf_s("%d", &a);
        PowerA3(a);
    }
    
    return 0;
}
