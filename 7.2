#include <stdio.h>
#include <locale.h>
#include <math.h>
#define M_PI  3.14159265358979323846;

int main()
{
    setlocale(LC_ALL,"Rus");
    double a, b;

    printf("Введите угол в радианах \n");
    scanf_s("%lf", &a);

    b = a * 180 / M_PI;

    printf("Угол в градусах равен %lf", b);
    return 0;
    
}
