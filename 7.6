#include <stdio.h>
#include <math.h>
#include <locale.h>

int main()
{
    setlocale(LC_ALL, "Rus");
    float x, a1, b1, c1, a2, b2, c2, y, d, d1, d2;

    printf("Введите а1 \n");
    scanf_s("%f", &a1);
    printf("Введите а2 \n");
    scanf_s("%f", &a2);
    printf("Введите b1 \n");
    scanf_s("%f", &b1);
    printf("Введите b2 \n");
    scanf_s("%f", &b2);
    printf("Введите c1 \n");
    scanf_s("%f", &c1);
